{
	"env": {
		"SDK_PATH": "${env:HOME}/zephyr-sdk"
	},
	"configurations": [
		{
			"name": "ZephyrRTOSConfiguration",
			"includePath": [
				"${workspaceFolder}/**",
				"${SDK_PATH}/xtensa-espressif_esp32_zephyr-elf/**"
			],
			"defines": [],
			"compilerPath": "${SDK_PATH}/xtensa-espressif_esp32_zephyr-elf/bin/xtensa-espressif_esp32_zephyr-elf-gcc",
			"cStandard": "c11",
			"cppStandard": "c++17",
			"IntelliSenseMode": "gcc-arm",
			"browse": {
				"path": [
					"${SDK_PATH}/xtensa-espressif_esp32_zephyr-elf/include/*",
					"${workspaceFolder}/*"
				],
				"limitSymbolsToIncludedHeaders": true
			}
		}
	],
	"version": 4
}